{
  "manifest_version": 3,
  "name": "Subdomain Switcher",
  "version": "1.0",
  "description": "Switch between different subdomains of the current URL",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}